   <!--Movimiento de auto green-->
   <script>
    // se guarda una variable con todas las propiedades de la camioneta
    var auto_green= document.getElementById('auto_green');

// unidad de tiempo en millisegundos
var unidad_tiempo_1 = 100;

// con cada unidad de tiempo, ¿cuanta distancia va a viajar el auto?
var distancia_auto = 5; // pixeles

// la posicion del auto
var posicion_auto = 0;

// llamando setInterval para repetir una funcion
setInterval(function(){
   if (posicion_auto < 0 || posicion_auto > 2700) {
       // cambiar el signo de la distancia
       distancia_auto = (-1)*distancia_auto; 
   }
   
   // dentro de la funcion, se usa la formula de distancia, velocidad y tiempo
   posicion_auto = posicion_auto + distancia_auto; // actualizando la posicion del auto
   auto_green.style.left = posicion_auto + 'px'; // cambiando la propiedad del auto 
},unidad_tiempo_1)
   </script>   

