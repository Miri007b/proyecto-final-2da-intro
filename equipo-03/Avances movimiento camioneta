var time=3000;
var velocity_camioneta=10;
var position_camioneta= 800;
var camioneta_black = document.getElementById('camioneta_black');
// llamando setInterval para repetir una funcion
setInterval(function(){
  position_camioneta=position_camioneta + -1* velocity_camioneta;
  camioneta_black.style.left=position_camioneta+"px"}
  ,200);
