<!--movimiento taxi yellow-->
   <script>
    // se guarda una variable con todas las propiedades del taxi
    var taxi_yellow = document.getElementById('taxi_yellow');

// unidad de tiempo en millisegundos
var unidad_tiempo_1 = 100;

// con cada unidad de tiempo, ¿cuanta distancia va a viajar el taxi?
var distancia_taxi = 5; // pixeles

// la posicion del taxi
var posicion_taxi = 0;

// llamando setInterval para repetir una funcion
setInterval(function(){
   if (posicion_taxi < 0 || posicion_taxi > 1700) {
       // cambiar el signo de la distancia
       distancia_taxi = (-1)*distancia_taxi; 
   }
   // dentro de la funcion, se usa la formula de distancia, velocidad y tiempo
   posicion_taxi = posicion_taxi + distancia_taxi; // actualizando la posicion del taxi
   taxi_yellow.style.left = posicion_taxi + 'px'; // cambiando la propiedad del taxi
},unidad_tiempo_1)
   </script>   


