<script>
//function camioneta_start() {//
  // se guarda una variable con todas las propiedades de la camioneta
var camioneta_black = document.getElementById('camioneta_black');

// unidad de tiempo en millisegundos
var unidad_tiempo_1 = 50;

// con cada unidad de tiempo, ¿cuanta distancia va a viajar la camioneta?
var distancia_camioneta = 10; // pixeles

// la posicion de la camioneta
var posicion_camioneta = 0;

//var counter_camioneta=0;//

// llamando setInterval para repetir una funcion
setInterval(function(){
    if (posicion_camioneta < 0, posicion_camioneta >= 800) {
        distancia_camioneta = (-1)*distancia_camioneta; // cambiar el signo de la distancia
      }
    if (posicion_camioneta < 0, posicion_camioneta >800) {
        
    }
    // dentro de la funcion, se usa la formula de distancia, velocidad y tiempo
    posicion_camioneta = posicion_camioneta + distancia_camioneta; // actualizando la posicion de la camioneta
    camioneta_black.style.left = posicion_camioneta + 'px'; // cambiando la propiedad de la camioneta
},unidad_tiempo_1)
//}//
//setInterval (camioneta_start,1000)//
</script> 


