<!--Movimientos de elementos-->

    <!--Movimiento de autobus-->
    <script>
      // se guarda una variable con todas las propiedades del auobus
      var autobus_sustentable= document.getElementById('autobus_sustentable');
  
  // unidad de tiempo en millisegundos
  var unidad_tiempo_1 = 100;
  
  // con cada unidad de tiempo, ¿cuanta distancia va a viajar el autobus?
  var distancia_autobus = 5; // pixeles
  
  // la posicion del autobus
  var posicion_autobus = 0;
  
  // llamando setInterval para repetir una funcion
  setInterval(function(){
     if (posicion_autobus < 0 || posicion_autobus > 2100) {
         // cambiar el signo de la distancia
         distancia_autobus = (-1)*distancia_autobus; 
     }
     // dentro de la funcion, se usa la formula de distancia, velocidad y tiempo
     posicion_autobus = posicion_autobus + distancia_autobus; // actualizando la posicion del autobus
     autobus_sustentable.style.left = posicion_autobus + 'px'; // cambiando la propiedad del autobus 
  },unidad_tiempo_1)
     </script>   


